<?xml version="1.0" encoding="UTF-8"?>    
<!DOCTYPE generatorConfiguration    
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"    
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">    
<generatorConfiguration> 
<!-- 数据库驱动-->       
    <classPathEntry  location="C:\Program Files (x86)\MySQL\Connector J 8.0\mysql-connector-java-8.0.29.jar"/>
    <context id="DB2Tables"  targetRuntime="MyBatis3">    
        <commentGenerator>    
            <property name="suppressDate" value="true"/>      
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>    
        <!--数据库链接URL，用户名、密码 -->   
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver" connectionURL="jdbc:mysql://43.142.182.70:3306/springboot_user?useUnicode=true&amp;characterEncoding=UTF-8&amp;serverTimezone=GMT%2B8" userId="root" password="zhaokedi">
        </jdbcConnection>    
        <javaTypeResolver>    
            <property name="forceBigDecimals" value="false"/>    
        </javaTypeResolver>    
        <!-- 生成模型的包名和位置-->
        <javaModelGenerator targetPackage="com.example.hotelmanagementbackgroud.model" targetProject="D:\workspace\hotel_management\hotel_backend\src\main\java">
            <property name="enableSubPackages" value="true"/>    
            <property name="trimStrings" value="true"/>    
        </javaModelGenerator>    
         <!-- 生成映射文件的包名和位置-->     
        <sqlMapGenerator targetPackage="mapper" targetProject="D:\workspace\hotel_management\hotel_backendsrc\main\resources">
            <property name="enableSubPackages" value="true"/>    
        </sqlMapGenerator>    
        <!-- 生成DAO的包名和位置-->    
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.hotelmanagementbackgroud.dao" targetProject="D:\workspace\hotel_management\hotel_backend\src\main\java">
            <property name="enableSubPackages" value="true"/>    
        </javaClientGenerator>    
        <!-- 要生成的表 tableName是数据库中的表名或视图名 domainObjectName是实体类名-->  
        <table tableName="myuser" domainObjectName="User" enableCountByExample="True" enableUpdateByExample="True" enableDeleteByExample="True" enableSelectByExample="True" selectByExampleQueryId="True"></table>
        <table tableName="datapro" domainObjectName="Data" enableCountByExample="True" enableUpdateByExample="True" enableDeleteByExample="True" enableSelectByExample="True" selectByExampleQueryId="True"></table>
        <table tableName="homestate" domainObjectName="homestate" enableCountByExample="True" enableUpdateByExample="True" enableDeleteByExample="True" enableSelectByExample="True" selectByExampleQueryId="True"></table>
    </context>
</generatorConfiguration> 